{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "command": {
            "type": "array"
        },
        "config": {
            "type": "object"
        },
        "fullnameOverride": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "nameOverride": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "pattern": {
            "type": "string"
        },
        "podAnnotations": {
            "type": "object"
        },
        "podSecurityContext": {
            "type": "object"
        },
        "rbac": {
            "type": "object",
            "properties": {
                "clusterRole": {
                    "type": "string"
                },
                "create": {
                    "type": "boolean"
                }
            }
        },
        "replicas": {
            "type": "integer"
        },
        "resources": {
            "type": "object"
        },
        "securityContext": {
            "type": "object",
            "properties": {
                "capabilities": {
                    "type": "object",
                    "properties": {
                        "add": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "stress": {
            "type": "object",
            "properties": {
                "cpuWorkers": {
                    "type": "integer"
                },
                "maxLoad": {
                    "type": "integer"
                },
                "timer": {
                    "type": "integer"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
